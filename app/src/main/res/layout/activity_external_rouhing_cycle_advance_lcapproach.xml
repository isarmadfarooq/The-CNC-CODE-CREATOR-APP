                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/s"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        android:background="@color/yellow"
                        android:textStyle="bold"/>
